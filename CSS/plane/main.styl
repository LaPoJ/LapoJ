$bg_corlor = #ffd200


*
  margin 0
  padding 0
html,body
  width 100%
  height 100%

.plane-page
  width 100%
  height 100%
  background-color $bg_corlor
  position relative
  .plane-wrapper
    position absolute
    left 20%
    top 40%
    animation undulate 8s linear infinite alternate
    perspective 2500px
    transition 1s
    .plane
      width 100%
    .rotate-container
      position absolute
      top 51.7003%
      left 13.1%
      display inline-block
      width 27.42%
      height 14.47%
      transform-origin: center center
      animation rataing .2s linear infinite reverse
      .propeller
        width 100%
        transform rotateY(-28deg)
@keyframes undulate
  0%,50%,100%
    transform translate3d(0, 0, 0)
  25%
    transform translate3d(-6px, -18px, 10px)
  75%
    transform translate3d(6px, 10px, 0)
@keyframes rataing
  0%
    transform rotateZ(0deg) rotateY(-30deg)
  50%
    transform rotateZ(180deg) rotateY(-30deg)
  100%
    transform rotateZ(360deg) rotateY(-30deg)
